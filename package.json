    "react-axe": "3.5.4",
    "react-router-dom": "5.2.1",
    "regenerator-runtime": "0.13.9",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.3.1",
