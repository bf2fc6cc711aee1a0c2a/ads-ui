{
  "name": "ads-ui",
  "federatedModuleName": "ads",
  "version": "0.0.1",
  "description": "The Red Hat OpenShift API Designer UI",
  "main": "server.js",
  "port": "9009",
  "repository": "https://github.com/bf2fc6cc711aee1a0c2a/ads-ui.git",
  "homepage": "https://cloud.redhat.com/application-services/api-designer",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "postinstall": "node .fix_yaml.js",
    "build": "webpack --config webpack.prod.js",
    "start": "node server.js",
    "start:dev": "webpack serve --hot --color --progress --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "ci-checks": "npm run type-check",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@apicurio/eslint-config": "^0.1.0",
